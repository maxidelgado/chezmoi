# This file is dynamically generated by chezmoi. DO NOT EDIT.

{{- $company := .company -}}
{{- if and (ne $company "personal") (lookPath "op") -}}
{{-   $item_name := printf "op://%s/aws-credentials" $company -}}
{{-   $awsCreds := onepasswordRead $item_name -}}

[default]
aws_access_key_id = {{ $awsCreds.access_key_id }}
aws_secret_access_key = {{ $awsCreds.secret_access_key }}

{{- else -}}
# No company specified or 1Password not found.
# Credentials for a personal account could be configured here if desired.
[default]
aws_access_key_id =
aws_secret_access_key =

{{- end -}}
